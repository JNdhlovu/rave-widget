{
    "name": "remade/rave-widget",
    "description": "A light-weight payment request library for Rave; for Laravel apps and composer aware PHP apps in general",
    "authors": [{
        "name": "remade",
        "email": "mail4remi@yahoo.com"
    }],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "catfan/medoo": "^1.5",
        "illuminate/support": "~5.5.0|~5.6.0",
        "nesbot/carbon": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Remade\\RaveWidget\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Remade\\RaveWidget\\Laravel\\RaveWidgetServiceProvider"
            ],
            "aliases": {
                "RaveWidget": "Remade\\RaveWidget\\Laravel\\RaveWidgetFacade"
            }
        }
    },
    "minimum-stability": "dev"

}
